{
  "name": "ctidigital/magento2-configurator",
  "description": "Keep magento persistently configured using files",
  "type":"magento2-module",
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "require": {
    "php": "~5.6.0|~7.0.0",
    "symfony/yaml": ">2.8.0",
    "magento/module-backend": "100.*.*",
    "magento/module-catalog": "100.*.*",
    "magento/module-eav": "100.*.*",
    "magento/module-layered-navigation": "100.*.*",
    "magento/framework": "100.*.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.1.0",
    "phpmd/phpmd": "@stable",
    "squizlabs/php_codesniffer": "1.5.3",
    "sebastian/phpcpd": "2.0.0",
    "satooshi/php-coveralls": "^1.0"
  },
  "version": "0.1.0-dev",
  "autoload": {
    "files": [ "registration.php" ],
    "psr-4": {
      "CtiDigital\\Configurator\\": ""
    }
  }
 }
